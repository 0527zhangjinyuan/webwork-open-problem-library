loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(1,5,1);
$b = non_zero_random(-5,5,1);
$c = random(-5,5,1);

$g_prime = FEQ("($a-$b +$b*x )*e^x /($a+$b*x)^2");

BEGIN_TEXT
Find the derivative of the function
\[ g(x) = \frac{e^x}{$a + ${b}x}  \]

$BR

\( g'(x) = \) \{ ans_rule(50) \}

END_TEXT

ANS(fun_cmp($g_prime));

