DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
"PG.pl",
"PGbasicmacros.pl",
"PGchoicemacros.pl",
"PGanswermacros.pl",
"PGauxiliaryFunctions.pl",
"MathObjects.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 0;

$a1 = random(1,5,1);
$a  = 2* $a1;
$s  = random(-1,1,2);
$b1 = random(10,40,1);
$b  = $s * $b1;
$c1 = random(1,5,1);
$c  = 1+2*$c1;

BEGIN_TEXT
$PAR
Evaluate the following limits.  If needed, enter 'INF' for \( \infty \) and
'-INF' for \( -\infty \).
END_TEXT

BEGIN_TEXT

$PAR
(a)
\(\displaystyle \lim_{ x \to  \frac{$c}{$a}^+ } \frac{ $b x }{ $c - $a x } = \)
\{ans_rule(10) \}
$PAR

(b)
\(\displaystyle \lim_{ x \to  \frac{$c}{$a}^- } \frac{ $b x }{ $c - $a x } = \)
\{ans_rule(10) \}

END_TEXT
if ($s == 1) {$ans1 = '-INF';} else {$ans1 = 'INF';}
if ($s == 1) {$ans2 = 'INF';} else {$ans2 = '-INF';}

ANS(Compute($ans1)->cmp);
ANS(Compute($ans2)->cmp);
#ANS(num_cmp($ans1, strings=>['INF','-INF']));
#ANS(num_cmp($ans2, strings=>['INF','-INF']));
ENDDOCUMENT();        # This should be the last executable line in the problem.

